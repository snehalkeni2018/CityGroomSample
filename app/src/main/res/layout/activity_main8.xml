<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
tools:context=".Main8Activity">

<android.support.design.widget.BottomNavigationView
    android:id="@+id/bottom_nav3"
    android:layout_width="match_parent"
    android:layout_height="56dp"
    android:layout_alignParentStart="true"
    android:layout_alignParentBottom="true"
    app:itemBackground="@color/colorBlack"
    app:itemIconTint="@color/colorWhite"
    app:menu="@menu/navigation_bar_items">

</android.support.design.widget.BottomNavigationView>

    <android.support.design.circularreveal.CircularRevealFrameLayout
        android:id="@+id/frame4"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/bottom_nav3"
        android:layout_alignParentStart="true"
        android:layout_marginStart="0dp"
        android:layout_marginBottom="-34dp">

        <View
            android:id="@+id/view"
            android:layout_width="400dp"
            android:layout_height="3dp"
            android:layout_marginTop="150dp"
            android:background="@color/colorBlack"
            />

        <Button
            android:id="@+id/button31"
            android:layout_width="125dp"
            android:layout_height="45dp"
            android:layout_marginTop="100dp"
            android:layout_marginLeft="230dp"
            android:text="VIEW PICTURES"
            android:background="@color/colorBlack"
            android:textColor="@color/colorWhite"/>
    </android.support.design.circularreveal.CircularRevealFrameLayout>


    <android.support.design.widget.NavigationView
        android:id="@+id/menu3"
        android:layout_width="66dp"
        android:layout_height="54dp"
        android:layout_gravity="center"
        android:background="@drawable/menu"
        app:menu="@menu/menu_items"></android.support.design.widget.NavigationView>

    <View
        android:id="@+id/view5"
        android:layout_width="match_parent"
        android:layout_height="54dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        android:background="@color/colorBlack" />

<ImageView
    android:id="@+id/imageView24"
    android:layout_width="229dp"
    android:layout_height="48dp"
    android:layout_alignParentStart="true"
    android:layout_alignParentTop="true"
    android:layout_marginStart="96dp"
    android:layout_marginTop="4dp"
    android:scaleType="centerCrop"
    app:srcCompat="@drawable/whitelogo" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="64dp"
        android:layout_marginEnd="115dp"
        android:text="COMPLAINTS"
        android:textColor="@color/colorBlack"
        android:textSize="17dp"
        android:textStyle="bold" />


    <ImageView
        android:id="@+id/imageView25"
        android:layout_width="48dp"
        android:layout_height="31dp"
        android:layout_alignStart="@+id/imageView20"
        android:layout_alignTop="@+id/textView13"
        android:layout_marginStart="90dp"
        android:layout_marginTop="60dp"
        app:srcCompat="@drawable/blackcomplaints" />


    <Button
        android:id="@+id/button18"
        android:layout_width="246dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/imageView20"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="49dp"
        android:layout_marginBottom="64dp"
        android:background="@color/colorBlack"
        android:text="RESOLVED POTHOLE PICTURES"
        android:textColor="@color/colorWhite" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="45dp"
        android:layout_height="51dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="14dp"
        android:layout_marginBottom="62dp"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/add" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="45dp"
        android:layout_height="51dp"
        android:layout_alignTop="@+id/button18"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="311dp"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/arrow" />


</RelativeLayout>