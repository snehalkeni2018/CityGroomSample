<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Main13Activity">

    <android.support.design.widget.BottomNavigationView
        android:layout_width="match_parent"
        android:layout_height="56dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        app:itemBackground="@color/colorBlack"
        app:itemIconTint="@color/colorWhite"
        app:menu="@menu/navigation_bar_items">

    </android.support.design.widget.BottomNavigationView>

    <android.support.design.widget.BottomNavigationView
        android:layout_width="63dp"
        android:layout_height="51dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="1dp"
        android:layout_marginTop="0dp"
        android:layout_marginBottom="457dp"
        app:itemBackground="@color/colorBlack"
        app:itemIconTint="@color/colorWhite"
        app:itemIconSize="45dp"
        app:menu="@menu/menu_icon">

    </android.support.design.widget.BottomNavigationView>

    <View
        android:id="@+id/view3"
        android:layout_width="match_parent"
        android:layout_height="54dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        android:background="@color/colorBlack" />

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="229dp"
        android:layout_height="48dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="96dp"
        android:layout_marginTop="4dp"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/whitelogo" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="45dp"
        android:layout_height="51dp"
        android:layout_alignTop="@+id/button18"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="311dp"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/arrow" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="51dp"
        android:layout_height="51dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="76dp"
        android:layout_marginTop="64dp"
        app:srcCompat="@drawable/notifications" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="133dp"
        android:layout_height="22dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="78dp"
        android:layout_toEndOf="@+id/imageView11"
        android:text="NOTIFICATIONS"
        android:textColor="@color/colorBlack"
        android:textSize="17dp"
        android:textStyle="bold" />


</RelativeLayout>